@Article{Voth-2005,
  Title                    = {Fast centroid molecular dynamics: a force-matching approach for the predetermination of the effective centroid forces.},
  Author                   = {Tyler D Hone and Sergei Izvekov and Gregory A Voth},
  Journal                  = {J Chem Phys},
