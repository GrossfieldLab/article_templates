  keywords    = {Capsid Proteins, chemistry; Computer Graphics; Micelles; Molecular Dynamics Simulation; Phospholipid Ethers, chemistry/pharmacology; Phosphorylcholine, analogs /&/ derivatives/chemistry/pharmacology; Protein Conformation, drug effects; Proteins, chemistry; Sodium Dodecyl Sulfate, chemistry/pharmacology; Software; User-Computer Interface},
  language    = {eng},
  medline-pst = {ppublish},
