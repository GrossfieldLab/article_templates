  Pmid                     = {19541654},
  Timestamp                = {2010.01.27},
  Url                      = {http://dx.doi.org/10.1073/pnas.0900072106}
}

@Article{Ernst-2008,
  Title                    = {Crystal structure of opsin in its {G}-protein-interacting conformation.},
  Author                   = {Patrick Scheerer and Jung Hee Park and Peter W Hildebrand and Yong Ju Kim and Norbert Krauss and Hui-Woog Choe and Klaus Peter Hofmann and Oliver P Ernst},
