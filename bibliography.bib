  Timestamp                = {2012.07.10},
  Url                      = {http://dx.doi.org/10.1021/la0475775}
}

@Article{Deleu-2009,
  Title                    = {Effect of cholesterol and fatty acids on the molecular interactions of fengycin with Stratum corneum mimicking lipid monolayers.},
  Author                   = {Eeman, M. and Francius, G. and DufrÃªne, Y. F. and Nott, K. and Paquot, M. and Deleu, M.},
