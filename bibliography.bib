  Keywords                 = {Acetyldigoxins, chemistry; Animals; Antibodies, Monoclonal, chemistry/immunology; Arginine, chemistry; Binding Sites; Crystallography, X-Ray; Digoxin, analogs /&/ derivatives/chemistry/immunology/pharmacology; Enzyme-Linked Immunosorbent Assay; Genetic Vectors; Haptens, chemistry; Kinetics; Mice; Models, Chemical; Mutagenesis, Site-Directed; Mutation; Protein Binding; Serum Albumin, chemistry; Valine, chemistry},
  Language                 = {eng},
  Medline-pst              = {ppublish},
