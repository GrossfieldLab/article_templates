  institution = {Preclinical Veterinary Sciences, R(D)SVS, College of Medicine and Veterinary Medicine, University of Edinburgh, Summerhall, Edinburgh EH9 1QH, UK. Mood@ed.ac.uk},
  keywords    = {Electrons; Hydrophobic and Hydrophilic Interactions; Lipid Bilayers, chemistry; Molecular Dynamics Simulation; Neutron Diffraction; Peptides, chemistry; Phosphatidylcholines, chemistry; Phosphatidylglycerols, chemistry; Surface Properties},
  language    = {eng},
