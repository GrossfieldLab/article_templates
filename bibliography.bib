@Article{Huber-2012a,
  Title                    = {Rhodopsin forms a dimer with cytoplasmic helix 8 contacts in native membranes.},
  Author                   = {Knepp, Adam M. and Periole, Xavier and Marrink, Siewert-Jan and Sakmar, Thomas P. and Huber, Thomas},
