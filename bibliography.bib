  issue           = {7},
  keywords        = {Allosteric Regulation; Allosteric Site; Amino Acid Sequence; Molecular Dynamics Simulation; Molecular Sequence Data; Protein Binding; Receptor, Muscarinic M3, chemistry, metabolism; Sodium, pharmacology},
  nlm             = {PMC4390834},
  nlm-id          = {0370626},
  owner           = {NLM},
