@Article{Khorana-1993,
  Title                    = {Formation of the meta {II} photointermediate is accompanied by conformational changes in the cytoplasmic surface of rhodopsin.},
  Author                   = {J. F. Resek and Z. T. Farahbakhsh and W. L. Hubbell and H. G. Khorana},
  Journal                  = {Biochemistry},
