  posted-at            = {2008-06-09 22:30:41},
  timestamp            = {2009.03.17},
  url                  = {http://dx.doi.org/10.1021/jp800687p},
}

@Article{Moore-2010,
  author =               {ter Haar, Ernst and Koth, Christopher M. and Abdul-Manan, Norzehan and Swenson, Lora and Coll, Joyce T. and Lippke, Judith A. and Lepre, Christopher A. and Garcia-Guzman, Miguel and Moore, Jonathan M.},
