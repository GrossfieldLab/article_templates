@Article{Huang-2005a,
  author               = {Lee, Ming-Tao and Hung, Wei-Chin and Chen, Fang-Yu and Huang, Huey W},
  title                = {Many-body effect of antimicrobial peptides: on the correlation between lipid's spontaneous curvature and pore formation.},
