  Timestamp                = {2009.07.01},
  Url                      = {http://dx.doi.org/10.1016/j.ssnmr.2009.03.003}
}

@Article{deKievit-2007,
  Title                    = {Molecular and biochemical detection of fengycin- and bacillomycin D-producing Bacillus spp., antagonistic to fungal pathogens of canola and wheat.},
