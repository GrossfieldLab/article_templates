  Keywords                 = {Protein Conformation, Neural Networks (Computer), Computer Simulation, Normal Distribution, Peptides, Motion, Enzyme Inhibitors, alpha-Amylase, Models: Molecular, Fluorescence Polarization},
  Language                 = {eng},
  Local-url                = {file://localhost/Users/tromo/Documents/Papers/2000/Doruker/Proteins%202000%20Doruker.pdf},
  Owner                    = {alan},
