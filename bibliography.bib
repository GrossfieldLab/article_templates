  Timestamp                = {2009.10.25}
}

@Article{Laoui-2007,
  author =               {Li, Youyong and Zhu, Fangqiang and Vaidehi, Nagarajan and Goddard, William A. and Sheinerman, Felix and Reiling, Stephan and Morize, Isabelle and Mu, Lan and Harris, Keith and Ardati, Ali and Laoui, Abdelazize},
  title =                {Prediction of the 3D Structure and Dynamics of Human DP G-Protein Coupled Receptor Bound to an Agonist and an Antagonist},
