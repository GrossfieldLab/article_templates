}

@Article{Ishiguro-1995,
  author     = {KOIDEL, M. and ISHIGURO, S.},
  journal    = {J. Solut. Chem.},
  title      = {MOLAR ENTHALPIES OF TRANSFER OF DIVALENT TRANSITION-METAL IONS AND THEIR CHLORO COMPLEXES FROM N,N-DIMETHYLFORMAMIDE TO N,N- DIMETHYLACETAMIDE},
  year       = {1995},
  pages      = {511-522},
  annotation = {The molar enthalpies of transfer Delta(t)H(o) of some divalent metal ions (M = Mn, Co, Ni and Zn) and their chloro complexes from N,N-dimethyl-formamide (DMF) to N,N-dimethylacetamide (DMA) have been determined using the tetraphenylarsonium- tetraphenylborate (TATB) assumption at 25 degrees C. Although physicochemical properties of DMF and DMA as solvent are similar, the Delta(t)H(o)(M(2+)) value increased significantly in the order Mn < Co < Ni < Zn. The corresponding Delta(t)H(o) values for the mono- and dichloro complexes showed also a strong metal dependence, while those for the tri- and tetrachloro complexes practically do not. These results can be reasonably explained in terms of steric hindrance upon solvation of the metal ions and complexes in DMA.},
}

@Article{Okamoto-2009,
  author               = {Kokubo, Hironori and Okamoto, Yuko},
  journal              = {Biophysical Journal},
  title                = {Analysis of Helix-Helix Interactions of Bacteriorhodopsin by Replica-Exchange Simulations},
  year                 = {2009},
  number               = {3},
  pages                = {765--776},
  volume               = {96},
  abstract             = {We performed long-time replica-exchange Monte Carlo simulations of bacteriorhodopsin transmembrane helices, which made it possible that wide conformational space was sampled. Using only the helix-helix interactions and starting from random initial configurations, we obtained the nativelike helix arrangement successfully and predicted a part of the configurations (three helices out of seven) precisely. By the principal component analysis we classified low-energy structures into some clusters of similar structures, and we showed that the above nativelike three-helix configuration is reproduced properly in most clusters and that not only the van der Waals interactions but also the electrostatic interactions contributed to the stabilization of the native structures},
