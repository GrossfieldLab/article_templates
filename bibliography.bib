  keywords        = {Alanine, chemistry; Algorithms; Methane, chemistry; Molecular Dynamics Simulation; Peptides, chemistry; Sodium Chloride, chemistry; Stochastic Processes; Time Factors; first-passage time; non-Markovian; rare event; weighted ensemble},
  month           = jan,
  nlm-id          = {9211750},
  owner           = {NLM},
