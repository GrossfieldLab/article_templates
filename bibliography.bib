@Article{DUrsi-2010,
  Title                    = {Synthesis of new antifungal peptides selective against Cryptococcus neoformans.},
  Author                   = {Grimaldi, Manuela and {De Rosa}, Margherita and {Di Marino}, Sara and Scrima, Mario and Posteraro, Brunella and Sanguinetti, Maurizio and Fadda, Giovanni and Soriente, Annunziata and D'Ursi, Anna Maria},
