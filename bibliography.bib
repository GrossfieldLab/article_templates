  Rating                   = {0},
  Timestamp                = {2010.01.20},
  Uri                      = {papers://D16675BD-70C8-46A5-A8B3-C40BD126B588/Paper/p6596},
  Url                      = {http://eproceedings.worldscinet.com/9789814295291/9789814295291_0029.html}
