@Article{Khandogin-2008,
  Title                    = {Recent advances in implicit solvent-based methods for biomolecular simulations.},
  Author                   = {Jianhan Chen and Charles L Brooks and Jana Khandogin},
  Journal                  = {Curr Opin Struct Biol},
  Year                     = {2008},

  Month                    = {Apr},
  Number                   = {2},
