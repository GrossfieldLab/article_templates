@Article{Woodcock-2008,
  Title                    = {{CHARMMing}: a new, flexible web portal for {CHARMM}},
  Author                   = {Miller, Benjamin T. and Singh, Rishi P. and Klauda, Jeffery B. and Hodoscek, Milan and Brooks, Bernard R. and Woodcock, 3rd, H Lee},
  Journal                  = {J Chem Inf Model},
