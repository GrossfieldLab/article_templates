}

@Article{Sachs-2009a,
  Title                    = {Curvature dynamics of alpha-synuclein familial Parkinson disease mutants: molecular simulations of the micelle- and bilayer-bound forms.},
  Author                   = {Jason D Perlmutter and Anthony R Braun and Jonathan N Sachs},
  Journal                  = {J Biol Chem},
  Year                     = {2009},

  Month                    = {Mar},
  Number                   = {11},
  Pages                    = {7177--7189},
  Volume                   = {284},

  Abstract                 = {Alpha-synuclein remains a protein of interest due to its propensity to form fibrillar aggregates in neurodegenerative disease and its putative function in synaptic vesicle regulation. Herein, we present a series of atomistic molecular dynamics simulations of wild-type alpha-synuclein and three Parkinson disease familial mutants (A30P, A53T, and E46K) in two distinct environments. First, in order to match recent NMR experiments, we have simulated each protein bound to an SDS detergent micelle. Second, in order to connect more closely to the true biological environment, we have simulated the proteins bound to a 1,2-dioleoyl-sn-glycero-3-phosphoserine lipid bilayer. In the micelle-bound case, we find that the wild type and all of the variants of alpha-synuclein flatten the underlying micelle, decreasing its surface area. A30P is known to lessen alpha-synuclein/membrane affinity and, consistent with experiment, destabilizes the simulated secondary structure. In the case of A53T, our simulations reveal a range of stabilizing hydrogen bonds that form with the threonine. In both environments, the E46K mutation, which is known to increase bilayer affinity, leads to an additional hydrogen bond between the protein and either the detergent or lipid. Simulations indicate that alphaS and its variants are less dynamic in the bilayer than in the micelle. Furthermore, the simulations of the mutants suggest how changes in the structure and dynamics of alpha-synuclein may affect its biological role.},
  Doi                      = {10.1074/jbc.M808895200},
  Institution              = {Department of Biomedical Engineering, University of Minnesota, Minneapolis, Minnesota 55455, USA.},
  Keywords                 = {Amino Acid Substitution; Computer Simulation; Humans; Lipid Bilayers, chemistry; Micelles; Models, Molecular; Mutation, Missense; Parkinsonian Disorders; Protein Structure, Secondary, genetics; Sodium Dodecyl Sulfate, chemistry; Structure-Activity Relationship; alpha-Synuclein, chemistry/genetics/metabolism},
  Language                 = {eng},
  Medline-pst              = {ppublish},
  Owner                    = {alan},
  Pii                      = {M808895200},
  Pmid                     = {19126542},
  Timestamp                = {2009.10.25},
  Url                      = {http://dx.doi.org/10.1074/jbc.M808895200}
}

@Article{Sachs-2009,
  Title                    = {Experimental verification of lipid bilayer structure through multi-scale modeling.},
  Author                   = {Jason D Perlmutter and Jonathan N Sachs},
  Journal                  = {Biochim Biophys Acta},
  Year                     = {2009},

  Month                    = {Oct},
  Number                   = {10},
  Pages                    = {2284--2290},
  Volume                   = {1788},

  Abstract                 = {Great progress has been made in applying coarse-grain molecular dynamics (CGMD) simulations to the investigation of membrane biophysics. In order to validate the accuracy of CGMD simulations of membranes, atomistic scale detail is necessary for direct comparison to structural experiments. Here, we present our strategy for verifying CGMD lipid bilayer simulations. Through reverse coarse graining and subsequent calculation of the bilayer electron density profile, we are able to compare the simulations to our experimental low angle X-ray scattering (LAXS) data. In order to determine the best match to the experimental data, atomistic simulations are run at a range of areas (in the NP(N)AT ensemble), starting from distinct configurations extracted from the CGMD simulation (run in the NPT ensemble). We demonstrate the effectiveness of this procedure with two small, single-component bilayers, and suggest that the greater utility of our algorithm will be for CGMD simulations of more complex structures.},
  Doi                      = {10.1016/j.bbamem.2009.07.006},
  Institution              = {Department of Biomedical Engineering, University of Minnesota - Twin Cities, 312 Church Street S.E., 7-126 Hasselmo Hall, Minneapolis, MN 55455, USA.},
