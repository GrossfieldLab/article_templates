@Article{Pomes-2013,
  author    = {Neale, Chris and Madill, Chris and Rauscher, Sarah and Pom{\`e}s, R{\'e}gis,},
  title     = {Accelerating Convergence in Molecular Dynamics Simulations of Solutes in Lipid Membranes by Conducting a Random Walk along the Bilayer Normal},
  journal   = {Journal of Chemical Theory and Computation},
  year      = {2013},
