@Article{Sunahara-2011,
  author          = {Chung, Ka Young and Rasmussen, SÃ¸ren G F and Liu, Tong and Li, Sheng and DeVree, Brian T and Chae, Pil Seok and Calinski, Diane and Kobilka, Brian K and Woods, Virgil L and Sunahara, Roger K},
  title           = {Conformational changes in the G protein Gs induced by the Î²2 adrenergic receptor.},
  journal         = {Nature},
  year            = {2011},
  volume          = {477},
  pages           = {611--615},
  month           = sep,
  issn            = {1476-4687},
  abstract        = {G protein-coupled receptors represent the largest family of membrane receptors that instigate signalling through nucleotide exchange on heterotrimeric G proteins. Nucleotide exchange, or more precisely, GDP dissociation from the G protein Î±-subunit, is the key step towards G protein activation and initiation of downstream signalling cascades. Despite a wealth of biochemical and biophysical studies on inactive and active conformations of several heterotrimeric G proteins, the molecular underpinnings of G protein activation remain elusive. To characterize this mechanism, we applied peptide amide hydrogen-deuterium exchange mass spectrometry to probe changes in the structure of the heterotrimeric bovine G protein, Gs (the stimulatory G protein for adenylyl cyclase) on formation of a complex with agonist-bound human Î²(2) adrenergic receptor (Î²(2)AR). Here we report structural links between the receptor-binding surface and the nucleotide-binding pocket of Gs that undergo higher levels of hydrogen-deuterium exchange than would be predicted from the crystal structure of the Î²(2)AR-Gs complex. Together with X-ray crystallographic and electron microscopic data of the Î²(2)AR-Gs complex (from refs 2, 3), we provide a rationale for a mechanism of nucleotide exchange, whereby the receptor perturbs the structure of the amino-terminal region of the Î±-subunit of Gs and consequently alters the 'P-loop' that binds the Î²-phosphate in GDP. As with the Ras family of small-molecular-weight G proteins, P-loop stabilization and Î²-phosphate coordination are key determinants of GDP (and GTP) binding affinity.},
  chemicals       = {Adrenergic beta-2 Receptor Agonists, Receptors, Adrenergic, beta-2, Guanosine Diphosphate, Guanosine Triphosphate, GTP-Binding Protein alpha Subunits, Gs},
