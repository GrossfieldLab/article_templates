@Article{Selent-2016,
  author =      {Guix{\`{a}}-Gonz{\'{a}}lez, Ramon and Javanainen, Matti and G{\'{o}}mez-Soler, Maricel and Cordobilla, Bego{\~{n}}a and Domingo, Joan Carles and Sanz, Ferran and Pastor, Manuel and Ciruela, Francisco and Martinez-Seara, Hector and Selent, Jana},
