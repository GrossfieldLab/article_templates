  issn-linking    = {0893-133X},
  journal         = {Neuropsychopharmacology : official publication of the American College of Neuropsychopharmacology},
  keywords        = {Analysis of Variance; Animals; Arrestins, deficiency, metabolism; Behavior, Animal, drug effects, physiology; Conditioning, Operant, drug effects, physiology; Dopamine Plasma Membrane Transport Proteins, deficiency; Enzyme Inhibitors, pharmacology; Extracellular Signal-Regulated MAP Kinases, metabolism; Female; Gene Expression Regulation, drug effects, genetics; Immunoprecipitation, methods; Male; Mice; Mice, Inbred C57BL; Mice, Knockout; Morphine, pharmacology; Motor Activity, drug effects, genetics; Narcotics, pharmacology; Psychomotor Performance, drug effects; Receptors, Dopamine D1, deficiency, metabolism; Reward; Signal Transduction, drug effects, genetics, physiology; Time Factors; beta-Arrestin 2; beta-Arrestins},
