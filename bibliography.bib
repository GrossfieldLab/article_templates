@Article{Grossfield-2006c,
  Title                    = {Retinal counterion switch mechanism in vision evaluated by molecular simulations.},
  Author                   = {Karina Mart\'{i}nez-Mayorga and Michael C Pitman and Alan Grossfield and Scott E Feller and Michael F Brown},
  Journal                  = {J Am Chem Soc},
  Year                     = {2006},

  Month                    = {Dec},
  Number                   = {51},
  Pages                    = {16502--16503},
  Volume                   = {128},

  Abstract                 = {Photoisomerization of the retinylidene chromophore of rhodopsin is the starting point in the vision cascade. A counterion switch mechanism that stabilizes the retinal protonated Schiff base (PSB) has been proposed to be an essential step in rhodopsin activation. On the basis of vibrational and UV-visible spectroscopy, two counterion switch models have emerged. In the first model, the PSB is stabilized by Glu181 in the meta I state, while in the most recent proposal, it is stabilized by Glu113 as well as Glu181. We assess these models by conducting a pair of microsecond scale, all-atom molecular dynamics simulations of rhodopsin embedded in a 99-lipid bilayer of SDPC, SDPE, and cholesterol (2:2:1 ratio) varying the starting protonation state of Glu181. Theoretical simulations gave different orientations of retinal for the two counterion switch mechanisms, which were used to simulate experimental 2H NMR spectra for the C5, C9, and C13 methyl groups. Comparison of the simulated 2H NMR spectra with experimental data supports the complex-counterion mechanism. Hence, our results indicate that Glu113 and Glu181 stabilize the retinal PSB in the meta I state prior to activation of rhodopsin.},
  Doi                      = {10.1021/ja0671971},
  Institution              = {Departments of Chemistry and Physics, University of Arizona, Tucson, AZ 85721, USA.},
  Keywords                 = {Computer Simulation; Magnetic Resonance Spectroscopy, methods; Models, Biological; Molecular Structure; Rhodopsin, chemical synthesis/chemistry; Schiff Bases, chemistry; Sensitivity and Specificity; Thermodynamics; Vision, Ocular},
  Language                 = {eng},
  Medline-pst              = {ppublish},
  Owner                    = {alan},
  Pmid                     = {17177390},
  Timestamp                = {2010.01.19},
  Url                      = {http://dx.doi.org/10.1021/ja0671971}
}

