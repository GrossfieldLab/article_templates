@Article{Almeida-2011b,
  Title                    = {A thermodynamic approach to the mechanism of cell-penetrating peptides in model membranes.},
  Author                   = {McKeown, Alesia N. and Naro, Jeffrey L. and Huskins, Laura J. and Almeida, Paulo F.},
  Journal                  = {Biochemistry},
  Year                     = {2011},

  Month                    = {Feb},
  Number                   = {5},
  Pages                    = {654--662},
  Volume                   = {50},

  Abstract                 = {We report a first test of the hypothesis that the mechanism of antimicrobial, cytolytic, and amphipathic cell-penetrating peptides in model membranes is determined by the thermodynamics of insertion of the peptide into the lipid bilayer from the surface-associated state. Three peptides were designed with minimal mutations relative to the sequence of TP10W, the Y3W variant of transportan 10, which is a helical, amphipathic cell-penetrating peptide previously studied. Binding to 1-palmitoyl-2-oleoylphosphatidylcholine (POPC) membranes and release of dye from those vesicles were assessed by stopped-flow fluorescence, and the secondary structure of the peptides on the membrane was determined by circular dichroism. The Gibbs energy of binding determined experimentally was in excellent agreement with that calculated using the Wimley-White interfacial hydrophobicity scale, taking into account the helical content of the membrane-associated peptide. Release of dye from POPC vesicles remained graded, as predicted by the hypothesis. More significantly, as the Gibbs energy of insertion into the bilayer became more unfavorable, which was estimated using the Wimley-White octanol hydrophobicity scale, dye release became slower, in quantitative agreement with the prediction.},
  Doi                      = {10.1021/bi1013358},
  Institution              = {Department of Chemistry and Biochemistry, University of North Carolina, Wilmington, North Carolina 28403, United States.},
  Keywords                 = {Amino Acid Sequence; Cell Membrane, metabolism; Cell-Penetrating Peptides, chemistry/genetics/metabolism; Kinetics; Lipid Bilayers, metabolism; Models, Biological; Molecular Sequence Data; Thermodynamics},
