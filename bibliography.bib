@Article{Almeida-2011,
  Title                    = {What determines the activity of antimicrobial and cytolytic peptides in model membranes.},
  Author                   = {Clark, Kim S. and Svetlovics, James and McKeown, Alesia N. and Huskins, Laura and Almeida, Paulo F.},
  Journal                  = {Biochemistry},
  Year                     = {2011},

  Month                    = {Sep},
  Number                   = {37},
  Pages                    = {7919--7932},
  Volume                   = {50},

  Abstract                 = {We previously proposed three hypotheses relating the mechanism of antimicrobial and cytolytic peptides in model membranes to the Gibbs free energies of binding and insertion into the membrane [Almeida, P. F., and Pokorny, A. (2009) Biochemistry 48, 8083-8093]. Two sets of peptides were designed to test those hypotheses, by mutating of the sequences of Î´-lysin, cecropin A, and magainin 2. Peptide binding and activity were measured on phosphatidylcholine membranes. In the first set, the peptide charge was changed by mutating basic to acidic residues or vice versa, but the amino acid sequence was not altered much otherwise. The type of dye release changed from graded to all-or-none according to prediction. However, location of charged residues in the sequence with the correct spacing to form salt bridges failed to improve binding. In the second set, the charged and other key residues were kept in the same positions, whereas most of the sequence was significantly but conservatively simplified, maintaining the same hydrophobicity and amphipathicity. This set behaved completely different from predicted. The type of release, which was expected to be maintained, changed dramatically from all-or-none to graded in the mutants of cecropin and magainin. Finally, contrary to the hypotheses, the results indicate that the Gibbs energy of binding to the membrane, not the Gibbs energy of insertion, is the primary determinant of peptide activity.},
  Doi                      = {10.1021/bi200873u},
  Institution              = {Department of Chemistry and Biochemistry, University of North Carolina, Wilmington, North Carolina 28403, USA.},
  Keywords                 = {Amino Acid Sequence; Animals; Anti-Infective Agents, chemistry/metabolism; Bacterial Proteins, chemistry/metabolism; Galanin, chemistry/metabolism; Hemolysin Proteins, chemistry/metabolism; Magainins, chemistry/metabolism; Membranes, Artificial; Molecular Sequence Data; Peptides, chemistry/genetics/metabolism; Phosphatidylcholines, chemistry/genetics/metabolism; Protein Binding; Protein Transport; Recombinant Fusion Proteins, chemistry/metabolism; Static Electricity; Thermodynamics; Wasp Venoms, chemistry/metabolism; Xenopus Proteins, chemistry/metabolism; Xenopus laevis},
  Language                 = {eng},
