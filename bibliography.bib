@Article{Heyn-2008,
  author               = {Hoersch, Daniel and Otto, Harald and Wallat, Ingrid and Heyn, Maarten P.},
  journal              = {Biochemistry},
  title                = {Monitoring the Conformational Changes of Photoactivated Rhodopsin from \&\#x39c;icroseconds to Seconds by Transient Fluorescence Spectroscopy},
  year                 = {2008},
  month                = {November},
  number               = {44},
  pages                = {11518--11527},
  volume               = {47},
  abstract             = {Abstract: The transient changes of the tryptophan fluorescence of bovine rhodopsin in ROS membranes were followed in time from 1 ÃÂ¼s to 10 s after flash excitation of the photoreceptor. Up to about 100 ÃÂ¼s the fluorescence did not change, suggesting that the tryptophan lifetimes in rhodopsin and the MI intermediate are similar. The fluorescence then decreases on the millisecond time scale with kinetics that match the rise of the MII state as measured on the same sample by the transient absorption increase at 360 nm. Both the sign and kinetics of the fluorescence change strongly suggest that it is due to an increase in energy transfer to the retinylidene chromophore caused by the increased spectral overlap in MII. Calculation of the Forster radius of each tryptophan from the high-resolution crystal structure suggests that W265 and W126 are already completely quenched in the dark, whereas W161, W175, and W35 are located at distances from the retinal chromophore that are comparable to their Forster radii. The fluorescence from these residues is thus sensitive to an increase in energy transfer in MII. Similar results were obtained at other temperatures and with monomeric rhodopsin in dodecyl maltoside micelles. A large light-induced transient fluorescence increase was observed with ROS membranes that were selectively labeled with Alexa594 at cysteine 316 in helix 8. Using transient absorption spectroscopy the kinetics of this structural change at the cytoplasmic surface was compared to the formation of the signaling state MII (360 nm) and to the kinetics of proton uptake as measured with the pH indicator dye bromocresol purple (605 nm). The fluorescence kinetics lags behind the deprotonation of the Schiff base. The proton uptake is even further delayed. These observations show that in ROS membranes (at pH 6) the sequence of events is Schiff base deprotonation, structural change, and proton uptake. From the temperature dependence of the kinetics we conclude that the Schiff base deprotonation and the transient fluorescence have comparable activation energies, whereas that of proton uptake is much smaller.},
  address              = {Biophysics Group, Department of Physics, Freie Universitat Berlin, Arnimallee 14, 14195 Berlin, Germany},
  citeulike-article-id = {3466496},
  doi                  = {http://dx.doi.org/10.1021/bi801397e},
  keywords             = {rhodopsin},
  owner                = {alan},
  posted-at            = {2008-10-30 18:55:00},
  timestamp            = {2009.03.17},
  url                  = {http://dx.doi.org/10.1021/bi801397e},
}

