@Article{Zuckerman-2010,
  Title                    = {Steady-state simulations using weighted ensemble path sampling.},
  Author                   = {Divesh Bhatt and Bin W Zhang and Daniel M Zuckerman},
  Journal                  = {J Chem Phys},
