  keywords        = {Analgesics, Opioid; Animals; Arrestins, agonists, metabolism; Enkephalin, Ala(2)-MePhe(4)-Gly(5)-, pharmacology; Enkephalins; Mice; Phosphorylation; Protein Transport; Receptors, Opioid, mu, metabolism; Ubiquitination; beta-Arrestins},
  month           = sep,
  nlm-id          = {2985121R},
  owner           = {NLM},
