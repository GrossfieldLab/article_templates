@Article{Palczewski-2009f,
  Title                    = {Topology of class A G protein-coupled receptors: insights gained from crystal structures of rhodopsins, adrenergic and adenosine receptors.},
  Author                   = {Debarshi Mustafi and Krzysztof Palczewski},
