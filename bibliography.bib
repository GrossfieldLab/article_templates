  pmid =        {1472495},
  timestamp =   {2015.06.26}
}

@Article{Ringel-2004,
  Title                    = {Determination of intracellular pH and compartmentation using diffusion-weighted NMR spectroscopy with pH-sensitive indicators.},
  Author                   = {Jack S Cohen and Menachem Motiei and Shani Carmi and Dana Shiperto and Odeya Yefet and Israel Ringel},
  Journal                  = {Magn Reson Med},
  Year                     = {2004},

  Month                    = {May},
  Number                   = {5},
