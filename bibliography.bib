@Article{Dufourc-2008,
  author               = {Jean-Francois, Frantz and Elezgaray, Juan and Berson, Pascal and Vacher, Pierre and Dufourc, Erick J.},
  journal              = {Biophys. J.},
  title                = {Pore formation induced by an antimicrobial peptide: electrostatic effects},
  year                 = {2008},
  month                = {September},
  pages                = {biophysj.108.136655+},
  abstract             = {We investigate the mode of action of cateslytin, an antimicrobial peptide (AMP), on zwitterionic biomembranes by performing numerical simulations and electrophysiological measurements on membrane vesicles. Using this natural beta-sheet AMP secreted during stress as a model we show that a single peptide is able to form a stable membrane pore of 1 nm diameter of 0.25 nS conductance found both from calculation and electrical measurements. The resulting structure does not resemble the 'barrel-stave' or 'carpet' models earlier predicted but is very close to that found in the simulation of alpha-helical peptides ([1]). Based on the simulation of a mutated peptide and the effects of small external electric fields, we conclude that electrostatic forces play a crucial role in the process of pore formation. 10.1529/biophysj.108.136655},
