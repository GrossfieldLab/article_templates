@Article{Jakobsson-2009,
  Title                    = {An improved united atom force field for simulation of mixed lipid bilayers.},
  Author                   = {See-Wing Chiu and Sagar A Pandit and H. L. Scott and Eric Jakobsson},
  Journal                  = {J Phys Chem B},
