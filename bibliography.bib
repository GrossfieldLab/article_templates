}

@Article{Radeke-2000,
  Title                    = {Selective inhibition of monocyte chemoattractant protein-1 gene expression in human embryonal kidney cells by specific triple helix-forming oligonucleotides.},
  Author                   = {P. Marchand and K. Resch and H. H. Radeke},
  Journal                  = {J Immunol},
  Year                     = {2000},

