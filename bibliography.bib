@Article{Hickel-2008,
  author               = {Pabst, Georg and Grage, Stephan and Danner-Pongratz, Sabine and Jing, Weiguo and Ulrich, Anne S. and Watts, Anthony and Lohner, Karl and Hickel, Andrea},
