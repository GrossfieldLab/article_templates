@Article{Na-2007,
  Title                    = {Coarse-graining of protein structures for the normal mode studies},
  Author                   = {Kilho Eom and Seung-Chul Baek and Jung-Hee Ahn and Sungsoo Na},
  Journal                  = {Journal of Computational Chemistry},
