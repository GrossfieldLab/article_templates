@Article{Faller-2008,
  author               = {Dickey, Allison N. and Faller, Roland},
  journal              = {Biophys. J.},
  title                = {Examining the Contributions of Lipid Shape and Headgroup Charge on Bilayer Behavior},
  year                 = {2008},
  month                = {May},
  pages                = {biophysj.107.128074+},
  abstract             = {To better understand bilayer property dependency on lipid electrostatics and headgroup size, we use atomistic Molecular Dynamics simulations to study negatively charged and neutral lipid membranes. We compare the negatively charged phosphatidic acid (PA), which at physiological pH and salt concentration has a negative spontaneous curvature, with the negatively charged phosphtidylglycerol (PG) and neutrally charged phosphatidylcholine (PC), both of which have zero spontaneous curvature. The PA lipids are simulated using two different sets of partial charges for the headgroup and the varied charge distribution between the two PA systems results in significantly different locations for the Na+ ions relative to the water/membrane interface. For one PA system, the Na+ ions are localized around the phosphate group. In the second PA system, the Na+ ions are located near the ester carbonyl atoms, which coincides with the preferred location site for the PG Na+ ions. We find that the Na+ ion location has a larger effect on bilayer fluidity properties than lipid headgroup size, where the Alipid and acyl chain order parameter values are more similar between the PA and PG bilayers that have Na+ ions located near the ester groups, than between the two PA bilayers. 10.1529/biophysj.107.128074},
  citeulike-article-id = {2857984},
  doi                  = {http://dx.doi.org/10.1529/biophysj.107.128074},
  keywords             = {membrane-electrostatics, membrane-forcefield},
