  citeulike-linkout-1 =  {http://dx.doi.org/10.1039/c2sm25075f},
  citeulike-linkout-2 =  {http://www.rsc.org/Publishing/Journals/article.asp?doi=c2sm25075f},
  doi =                  {10.1039/c2sm25075f},
  keywords =             {lipopeptide},
